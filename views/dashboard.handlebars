                     <!--
                       Dropdown menu, show/hide based on menu state.
       
                       Entering: "transition ease-out duration-100"
                         From: "transform opacity-0 scale-95"
                         To: "transform opacity-100 scale-100"
                       Leaving: "transition ease-in duration-75"
                         From: "transform opacity-100 scale-100"
                         To: "transform opacity-0 scale-95"
                     -->
                     
                     <div id="user-menu-dropdown" class="hidden absolute right-0 z-10 mt-2 w-48 origin-top-right rounded-md bg-white py-1 shadow-lg ring-1 ring-black ring-opacity-5 focus:outline-none" role="menu" aria-orientation="vertical" aria-labelledby="user-menu-button" tabindex="-1">
                         <div :class="`origin-top-right absolute right-0 mt-2 w-48 rounded-md shadow-lg transition ease-${dropdown ? 'out' : 'in'} duration-${dropdown ? '100' : '75'} transform opacity-${dropdown ? '100' : '0'} scale-${dropdown ? '100' : '95'}`">
                         </div>
     
                         <script>
                             // Get references to the button and dropdown menu
                             const button = document.getElementById("user-menu-button");
                             const dropdown = document.getElementById("user-menu-dropdown");
                         
                             // Add a click event listener to the button
                             button.addEventListener("click", function() {
                                 // Toggle the 'hidden' class on the dropdown menu
                                 dropdown.classList.toggle("hidden");
                             });
                         </script>
     
                         
